<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DeviceClient.Views.TelemetryModelPage"
             Title="Telemetry">
    <VerticalStackLayout Spacing="10" Margin="5">
        <Label 
           x:Name="Version"
           Text="{Binding Version}"
           FontSize="14" TextColor="Silver" />
        <Label 
           x:Name="Created"
           Text="{Binding Created}"
            FontSize="22" />
        <Label 
           x:Name="TypeOfData"
           Text="{Binding TypeOfData}"
            FontSize="22" />
        <Label 
           x:Name="TelemetryDataString"
           Text="{Binding TelemetryDataString}"
            FontSize="22" />
        <Button Text="Done" Clicked="DoneButton_Clicked" />
    </VerticalStackLayout>
</ContentPage>
